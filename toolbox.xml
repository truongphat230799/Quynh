<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category id="rotary" name="ROTARY" colour="#A39400" icon="spinner">
    <block type="initialise_rotary_encoder"></block>
    <block type="get_value"></block>
    <block type="set_range_value">
      <value name="min">
          <shadow type="math_number"><field name="NUM">000</field></shadow>
      </value>
      <value name="max">
          <shadow type="math_number"><field name="NUM">100</field></shadow>
      </value>
    </block>
    <block type="rotary_mode"></block>
    <block type="set_current_value">
      <value name="value">
          <shadow type="math_number"><field name="NUM">000</field></shadow>
      </value>
    </block>
  </category>
</xml>